{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "final-project-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "final",
            "image": "679907953987.dkr.ecr.us-east-2.amazonaws.com/group11-app:latest",
            "portMappings": [
                {
                    "name": "ngnix-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::679907953987:role/ecsTaskExecutionRole"
} 